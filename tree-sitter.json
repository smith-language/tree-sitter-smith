{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "smith",
      "camelcase": "Smith",
      "title": "Smith",
      "scope": "source.smith",
      "file-types": [
        "smith"
      ],
      "injection-regex": "^smith$",
      "class-name": "TreeSitterSmith"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Smith grammar for tree-sitter",
    "authors": [
      {
        "name": "Adam Kowalski",
        "email": "adam.kowalski.work@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-smith"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}